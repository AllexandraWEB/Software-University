# ============================
# Node / Express.js .gitignore
# ============================

# Node modules
node_modules/

# Dependency logs
npm-debug.log*
yarn-debug.log*
pnpm-debug.log*

# Build artifacts
dist/
build/
tmp/
temp/

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Logs
logs/
*.log
*.log.*

# Coverage reports
coverage/
.nyc_output/

# OS generated files
.DS_Store
Thumbs.db

# Editor directories and files
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln

# TypeScript build info (if using TS)
*.tsbuildinfo

# Archives
*.tgz
*.zip
*.tar

# Runtime files
pids
*.pid
*.seed
*.pid.lock

# Optional npm cache directory
.npm/

# Optional eslint cache
.eslintcache

# Optional REPL history
.node_repl_history

# Optional dotenv backups
.env.backup
